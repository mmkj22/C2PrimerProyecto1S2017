                    :}
              | texto:tex
                    {:
                        NodoHK nodo = new NodoHK("cadena");
                        nodo.hijos.add(new NodoHK(tex));
                        RESULT = nodo;
                    :}
              
              | par_izq ARITMETICAS:log par_der
                    {:
                        RESULT = log;
                    :}
              
              | LLAMADA_MET:nodo
                    {:
                        RESULT = nodo;
                    :};


