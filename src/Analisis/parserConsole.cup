                        RESULT = nodo;
                    :}
                |identificador:id
                    {:
                        NodoHK nodo = new NodoHK("identificador");
                        nodo.hijos.add(new NodoHK(id, idleft, idright));
                        RESULT = nodo;
                    :}
                |porcentaje
                    {:
